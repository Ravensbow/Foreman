@page "/personal/dashboard"
@inject IHttpClientFactory httpClientFactory
@inject Services.AppStateService AppState

<MudText Typo="Typo.h5" Color="Color.Primary" Class="mb-4">Dashboard</MudText>

<Foreman.Client.Pages.Category.CategoryList CategoryId="null"></Foreman.Client.Pages.Category.CategoryList>

@code
{
    protected override async Task OnInitializedAsync()
    {
        await AppState.SetBreadcrumbs(new List<BreadcrumbItem> { new BreadcrumbItem("Dashboard", "personal/dashboard") });
    }
}